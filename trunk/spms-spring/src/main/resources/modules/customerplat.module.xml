			<module name="guaranteeForm" icon="customer_05_01">
				<feature>
					REFRESH,VIEW,ADD,MODIFY,DELETE,PUBLISH,CANCELPUBLISH,MESSAGE,LOG,PRINT,EXPORT,SENIORSEARCH
