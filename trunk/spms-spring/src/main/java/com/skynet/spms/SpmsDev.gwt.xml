<?xml version="1.0" encoding="UTF-8"?>

<module rename-to="spms">

	<inherits name="com.smartgwtpro.SmartGwtProNoScript"/>  
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="gwtupload.GWTUpload"/>
    <stylesheet src="Upload.css"/>
    <!-- complic -->
    <set-property name="user.agent" value="gecko1_8"/>
	<!-- log -->
	<inherits name="com.google.gwt.logging.Logging"/>
    <set-property name="gwt.logging.logLevel" value="FINE"/>
    <set-property name="gwt.logging.popupHandler" value="DISABLED" />
    <!-- debug -->
	<inherits name="com.google.gwt.user.Debug"/>
  	<set-property name="gwt.enableDebugId" value="true"/>
   
    <!-- theme -->
    <inherits name="com.smartclient.theme.enterpriseblue.EnterpriseBlueResources"/>
    <!-- inherits name="com.smartclient.theme.graphite.Graphite"/-->
    <entry-point class='com.skynet.spms.client.SpmsMain'/>
    <!--  source path='client'/ -->
    
    <!-- i18n -->
    <extend-property name="locale" values="zh_CN"/>
	<extend-property name="locale" values="en"/>
	<set-property-fallback name="locale" value="zh_CN"/>
    
</module>
