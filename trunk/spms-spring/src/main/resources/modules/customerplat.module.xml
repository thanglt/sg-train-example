			</module>

			<!-- 客户送修申请 -->
			<module name="repairSheet" icon="customer_02_01">
				<feature>
					ADD,DELETE,MODIFY,VIEW,PUBLISH,CANCELPUBLISH,MESSAGE,LOG,PRINT,EXPORT,IMPORT,SENIORSEARCH
				</feature>
